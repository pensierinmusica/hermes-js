{
  "env": { "jest": true },
  "extends": "standard",
  "rules": {
    "no-console": "error",
    "no-extra-semi": "error",
    "no-var": "error",
    "padded-blocks": "off",
    "semi": ["error", "always"],
    "space-before-function-paren": ["error", "always"],
    "symbol-description": "off"
  }
}
